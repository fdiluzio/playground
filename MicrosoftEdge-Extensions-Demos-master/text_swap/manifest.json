{
    "name": "Text Swap",
    "version": "1.0",
    "author": "Microsoft",

    "icons": {
        "20": "icon_20.png"
    },


    "browser_action": {
        "default_icon": {
            "20": "icon_20.png",
            "40": "icon_40.png"
        }
    },

    "content_scripts": [{
        "matches": ["*://developer.microsoft.com/*"],
        "js": ["content.js"]
    }, {
        "matches": ["*://developer.microsoft.com/*/microsoft-edge/*"],
        "js": ["content-size.js"]
    }]
}
